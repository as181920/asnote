<br/>

<%= label_tag("label[format]", "format") %>
<%= select_tag("label[format]", options_for_select(["","String","Text","RichText","Date","Time","Number","Select","MultiSelect"], @label["format"])) %>
<br/>

<%= label_tag("label[select]", "select_items(splited by space)") %><BR/>
