<%= form_tag "/users" do %>
  <%= label_tag(:email, "email") %> <%= text_field_tag(:email, "") %>
  <br/>
  <%= label_tag(:password, "password") %> <%= text_field_tag(:password, "") %>
  <br/>
  <%= label_tag(:password_confirmation, "password_confirmation") %> <%= text_field_tag(:password_confirmation, "") %>
  <br/>
  <%= submit_tag("new") %>
<% end %>
