<%= form_tag "/notes" do %>
  <%= label_tag("note[name]", "name") %> <%= text_field_tag("note[name]","") %>
  <br/>
  <%= label_tag("note[comment]", "comment") %> <%= text_area_tag("note[comment]", "") %>
  <br/>
  <%= label_tag("note[permission]", "permission") %>
  <%= select_tag("note[permission]", options_for_select(["Default","Team","personal","private"])) %>
  <br/>
  <%= submit_tag("new") %>
<% end %>

